https://practice.geeksforgeeks.org/problems/need-some-change/1/?track=module-2-arrays-and-strings-java&batchId=114#


class Solution
{
    public void swapElements(int[] arr, int n)
    {
        // Code here
int temp;

for (int i = 0; i < arr.length - 2; i++)
{
temp = arr[i];
arr[i] = arr[i+2];
arr[i+2] = temp;
}
    }
}
