class Geeks{
    
    static void logicOp(boolean a, boolean b){
        /*output (a&&b), (a||b), and ((!a)&&(!b))separated by spaces*/
        System.out.print((a && b) + " " + (a || b) + " " + (!a && !b));
    }
}
