class Geeks {
    static void countOddEven(int a[], int n) {

        // Your code here
        int odd=0;
        int even=0;
        for (int i:a)
        {
            if (i%2==0)
            even++;
            else
            odd++;
        }
        
        System.out.print(odd+" "+even);
    }
}
