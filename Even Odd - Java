class Geeks {
    // 
	 static void evenOdd (int a,int b) 
	 {
		//Your code here
		if (a%2==0)
		{
		    System.out.println(a);
		    System.out.println(b);
		}
		else
		{
		   System.out.println(b);
		   System.out.println(a); 
		}
		
	     
	 }
}
